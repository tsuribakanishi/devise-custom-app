<h1>Tickets#index</h1>
<p>Find me in app/views/tickets/index.html.erb</p>


<table>
  <thead>
    <tr>
      <th>Ticket</th>
      <th>Book name</th>
      <th>Author</th>
      <th>Kbn1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tickets.each do |ticket| %>
      <tr>
        <td><%= ticket.tiket_id %></td>
        <td><%= ticket.book_id %></td>
        <td><%= ticket.user_id %></td>
        <td><%#= ticket.author %></td>
        <td><%#= ticket.kbn1 %></td>
        <td><%#= link_to 'Show', ticket %></td>
        <td><%#= link_to 'Edit', edit_book_path(ticket) %></td>
        <td><%#= link_to 'Destroy', ticket, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket', new_ticket_path %>
